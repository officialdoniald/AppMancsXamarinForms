<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppMancsXamarinForms.NotPrimaryPages.OtherPage"
             Title="Other">
    <ContentPage.Content>
        <StackLayout
                Style="{StaticResource NormalStackLayout}">
            <Button
                Margin="0,25,0,0"
                Text="Delete my account"
                Style="{StaticResource NormalButton}"
                x:Name="deleteAcoountPageButton"
                Clicked="deleteAcoountPageButton_ClickedAsync"/>
            <Button
                Margin="0,25,0,0"
                Text="Logout"
                Style="{StaticResource NormalButton}"
                x:Name="loguotButton"
                Clicked="loguotButton_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>